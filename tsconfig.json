{
"compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "noUnusedLocals": true,
    "lib": ["es2017","dom"],

    "jsx": "react",
    "jsxFactory": "h",

    "esModuleInterop":true
},
"include": [
    "**/*"
],
"exclude": [
    "node_modules/*",
    "**/*.spec.ts",
    "depr/*"
]}
